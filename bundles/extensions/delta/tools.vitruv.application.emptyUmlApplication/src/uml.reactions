//import "http://www.eclipse.org/papyrus/sysml/1.4/SysML/Activities" as sysmlActivities 
//import "http://www.eclipse.org/papyrus/sysml/1.4/SysML/Allocations" as sysmlAllocations 
//import "http://www.eclipse.org/papyrus/sysml/1.4/SysML/Blocks" as sysmlBlocks
//import "http://www.eclipse.org/papyrus/sysml/1.4/SysML/ConstraintBlocks" as sysmlConstraintBlocks
//import "http://www.eclipse.org/papyrus/sysml/1.4/SysML/DeprecatedElements" as sysmlDepracatedElements
//import "http://www.eclipse.org/papyrus/sysml/1.4/SysML/PortsAndFlows" as sysmlPortsAndFlows
//import "http://www.eclipse.org/papyrus/sysml/1.4/SysML/ModelElements" as sysmlModelElements
//import "http://www.eclipse.org/papyrus/sysml/1.4/SysML/Requirements" as sysmlRequirements 

reactions: UmlTest
in reaction to changes in UML
execute actions in UML
reaction Test { 
	after element uml::Model created and inserted in uml::Model[nestedPackage] 
	call {
		doTestThing(newValue);
	}	
}

routine doTestThing(uml::Model model) {
	action {
		update model {model.name = "test";}
		
		
	}
}

